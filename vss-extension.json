{
    "manifestVersion": 1,
    "id": "get-x-master-snippets",
    "version": "1.0.0",
    "publisher": "get-x-master",
    "name": "GetX Master Snippets for Flutter",
    "description": "An extension to accelerate the process of developing applications with flutter, aimed at everyone using the GetX Master package.",
    "public": true,
    "icons": {
        "default": "images/icon.png"
    },
    "categories": [
        "Azure Repos"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "scopes": [
        "vso.work_write"
    ],
    "tags": [
        "dart",
        "flutter",
        "get",
        "getx",
        "getxmaster",
        "state-management"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "screenshots": [
        {
            "path": "examples/getcontroller.gif"
        }
    ],
    "contributions": [
        {
            "id": "my-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "name": "My Hub",
                "uri": "my-hub.html"
            }
        }
    ],
    "files": [
        {
            "path": "my-hub.html",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}